{
  "name": "critical-presentation",
  "version": "1.0.0",
  "description": "first presentation",
  "main": "webpack.config.babel.js",
  "author": "fr_ench",
  "license": "MIT",
  "scripts": {
    "start": "webpack-dev-server",
    "Development": "webpack --dev",
    "Production": "webpack --live",
    "watch": "webpack --dev --watch",
    "postDevelopment": "npm run critical",
    "postProduction": "npm run critical & npm run minify",
    "postwatch": "npm run critical",
    "minify": "postcss -u cssnano --cssnano.autoprefixer.remove false --cssnano.safe true --cssnano.discardComments.removeAll true -r dist/app.css",
    "critical": "postcss -u postcss-critical-split --postcss-critical-split.output critical -u cssnano --cssnano.autoprefixer.remove false --cssnano.safe true --cssnano.discardComments.removeAll true -o dist/critical.css dist/app.css"
  },
  "dependencies": {
    "autoprefixer": "6.3.6",
    "babel": "^6.5.2",
    "babel-cli": "6.9.0",
    "babel-core": "6.9.1",
    "babel-es6-polyfill": "1.0.1",
    "babel-loader": "6.2.4",
    "babel-plugin-transform-object-assign": "6.8.0",
    "babel-plugin-transform-proto-to-assign": "6.9.0",
    "babel-polyfill": "6.9.1",
    "babel-preset-es2015": "6.9.0",
    "babel-preset-es2015-loose": "7.0.0",
    "babel-register": "6.9.0",
    "cheerio": "0.20.0",
    "chosen-npm": "1.4.2",
    "clean-webpack-plugin": "0.1.9",
    "css-loader": "0.23.1",
    "cssnano": "3.6.2",
    "custom-loader": "0.1.1",
    "exports-loader": "0.6.3",
    "expose-loader": "0.7.1",
    "extract-text-webpack-plugin": "1.0.1",
    "fg-loadcss": "1.2.0",
    "file-loader": "0.8.5",
    "highlightjs": "^9.8.0",
    "imports-loader": "0.6.5",
    "less": "2.7.1",
    "less-loader": "2.2.3",
    "loader-utils": "0.2.15",
    "postcss-cli": "2.6.0",
    "postcss-critical-split": "2.1.2",
    "postcss-flexbugs-fixes": "2.0.0",
    "postcss-initial": "1.5.2",
    "postcss-loader": "0.9.1",
    "reveal": "^0.0.4",
    "shower": "^2.1.4",
    "shower-core": "^2.0.7",
    "shower-ribbon": "^2.0.8",
    "style-loader": "0.13.1",
    "svg-defs-loader": "0.1.0",
    "svg-sprite-loader": "0.0.21",
    "svgo-loader": "1.1.0",
    "url-loader": "0.5.7",
    "webpack": "1.13.1",
    "yargs": "4.7.1"
  },
  "devDependencies": {
    "webpack-dev-server": "1.14.0"
  }
}
